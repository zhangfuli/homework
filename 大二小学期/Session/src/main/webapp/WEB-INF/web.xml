<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app
        xmlns="http://xmlns.jcp.org/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
        version="3.1">
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>com.kms.servlet.ListServlet</servlet-name>
    <servlet-class>com.kms.servlet.ListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>com.kms.servlet.ListServlet</servlet-name>
    <url-pattern>/getList</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>com.kms.controller.MySessionServlet</servlet-name>
    <servlet-class>com.kms.controller.MySessionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>com.kms.controller.MySessionServlet</servlet-name>
    <url-pattern>/mySession</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>com.kms.controller.Test</servlet-name>
    <servlet-class>com.kms.controller.Test</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>com.kms.controller.Test</servlet-name>
    <url-pattern>/test</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>com.kms.controller.SidServlet</servlet-name>
    <servlet-class>com.kms.controller.SidServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>com.kms.controller.SidServlet</servlet-name>
    <url-pattern>/sid.servlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>com.kms.controller.ReadMessageServlet</servlet-name>
    <servlet-class>com.kms.controller.ReadMessageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>com.kms.controller.ReadMessageServlet</servlet-name>
    <url-pattern>/message</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>com.kms.controller.MessageServlet</servlet-name>
    <servlet-class>com.kms.controller.MessageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>com.kms.controller.MessageServlet</servlet-name>
    <url-pattern>/message.servlet</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>com.kms.controller.LoginIndexServlet</servlet-name>
    <servlet-class>com.kms.controller.LoginIndexServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>com.kms.controller.LoginIndexServlet</servlet-name>
    <url-pattern>/loginIndex.servlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>com.kms.controller.LoginServlet</servlet-name>
    <servlet-class>com.kms.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>com.kms.controller.LoginServlet</servlet-name>
    <url-pattern>/login.servlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>com.kms.controller.OutServlet</servlet-name>
    <servlet-class>com.kms.controller.OutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>com.kms.controller.OutServlet</servlet-name>
    <url-pattern>/out.servlet</url-pattern>
  </servlet-mapping>

</web-app>
